{
  "name": "Tab Counteroo",
  "version": "1.0.8",
  "manifest_version": 2,
  "description": "Shows and logs a count of currently opened browser tabs.",
  "browser_action": {
  	"default_name": "Tab Counteroo",
    "default_icon": "icon48.png",
	"default_popup": "popup.html"
  },
	"background": {
		"scripts": ["idb-keyval.js",
                            "prototype.js",
                            "scripts.js",
                            "bg.js"]
	},
  "icons": {
	"16": "icon16.png",
	"32": "icon32.png",
    "48": "icon48.png",
	"128": "icon128.png"
  },
  "permissions": [
    "tabs"
  ]
}
